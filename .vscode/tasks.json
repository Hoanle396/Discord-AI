{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start Discord Health Bot (Development)",
			"type": "shell",
			"command": "pnpm",
			"args": [
				"run",
				"dev"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": [],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "new",
				"showReuseMessage": true
			},
			"options": {
				"cwd": "${workspaceFolder}"
			}
		}
	]
}